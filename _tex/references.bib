@article{jainRapidMagneticResonance2011,
  title = {Rapid Magnetic Resonance Measurement of Global Cerebral Metabolic Rate of Oxygen Consumption in Humans during Rest and Hypercapnia},
  author = {Jain, Varsha and Langham, Michael C and Floyd, Thomas F and Jain, Gaurav and Magland, Jeremy F and Wehrli, Felix W},
  year = {2011},
  month = jul,
  journal = {Journal of Cerebral Blood Flow \& Metabolism},
  volume = {31},
  number = {7},
  pages = {1504--1512},
  publisher = {SAGE Publications Ltd STM},
  issn = {0271-678X},
  doi = {10.1038/jcbfm.2011.34},
  urldate = {2024-09-26},
  abstract = {The effect of hypercapnia on cerebral metabolic rate of oxygen consumption (CMRO2) has been a subject of intensive investigation and debate. Most applications of hypercapnia are based on the assumption that a mild increase in partial pressure of carbon dioxide has negligible effect on cerebral metabolism. In this study, we sought to further investigate the vascular and metabolic effects of hypercapnia by simultaneously measuring global venous oxygen saturation (SvO2) and total cerebral blood flow (tCBF), with a temporal resolution of 30 seconds using magnetic resonance susceptometry and phase-contrast techniques in 10 healthy awake adults. While significant increases in SvO2 and tCBF were observed during hypercapnia (P {$<$} 0.005), no change in CMRO2 was noted (P {$>$} 0.05). Additionally, fractional changes in tCBF and end-tidal carbon dioxide (R2 = 0.72, P {$<$} 0.005), as well as baseline SvO2 and tCBF (R2 = 0.72, P {$<$} 0.005), were found to be correlated. The data also suggested a correlation between cerebral vascular reactivity (CVR) and baseline tCBF (R2 = 0.44, P = 0.052). A CVR value of 6.1\% {\textpm} 1.6\%/mm Hg was determined using a linear-fit model. Additionally, an average undershoot of 6.7\% {\textpm} 4\% and 17.1\% {\textpm} 7\% was observed in SvO2 and tCBF upon recovery from hypercapnia in six subjects.},
  langid = {english},
  file = {/home/weberam2/Dropbox/AssistantProf_BCCHRI/Zotero_Obsidian/Zotero/Jain_et_al_2011_Rapid_magnetic_resonance_measurement_of_global_cerebral_metabolic_rate_of.pdf}
