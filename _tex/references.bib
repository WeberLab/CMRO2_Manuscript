@article{luQuantitativeEvaluationOxygenation2008,
  title = {Quantitative Evaluation of Oxygenation in Venous Vessels Using {{T2-Relaxation-Under-Spin-Tagging MRI}}},
  author = {Lu, Hanzhang and Ge, Yulin},
  year = {2008},
  journal = {Magnetic Resonance in Medicine},
  volume = {60},
  number = {2},
  pages = {357--363},
  issn = {1522-2594},
  doi = {10.1002/mrm.21627},
  urldate = {2024-09-26},
  abstract = {Noninvasive measurement of cerebral venous oxygenation can serve as a tool for better understanding fMRI signals and for clinical evaluation of brain oxygen homeostasis. In this study a novel technique, T2-Relaxation-Under-Spin-Tagging (TRUST) MRI, is developed to estimate oxygenation in venous vessels. This method uses the spin labeling principle to automatically isolate pure blood signals from which T2 relaxation times are determined using flow-insensitive T2-preparation pulses. The blood T2 is then converted to blood oxygenation using a calibration plot. In vivo experiments gave a baseline venous oxygenation of 64.8 {\textpm} 6.3\% in sagittal sinus in healthy volunteers (n = 24). Reproducibility studies demonstrated that the standard deviation across trials was 2.0 {\textpm} 1.1\%. The effects of repetition time and inversion time selections were investigated. The TRUST technique was further tested using various physiologic challenges. Hypercapnia induced an increase in venous oxygenation by 13.8 {\textpm} 1.1\%. On the other hand, caffeine ingestion resulted in a decrease in oxygenation by 7.0 {\textpm} 1.8\%. Contrast agent infusion (Gd-DTPA, 0.1 mmol/kg) reduced venous blood T2 by 11.2 ms. The results of this study show that TRUST MRI is a useful technique for quantitative assessment of blood oxygenation in the brain. Magn Reson Med 60:357--363, 2008. {\copyright} 2008 Wiley-Liss, Inc.},
  copyright = {Copyright {\copyright} 2008 Wiley-Liss, Inc.},
  langid = {english},
  keywords = {baseline condition,fMRI,hypercapnia,oxygenation,Trust MRI},
  file = {/home/weberam2/Dropbox/AssistantProf_BCCHRI/Zotero_Obsidian/Zotero/Lu_Ge_2008_Quantitative_evaluation_of_oxygenation_in_venous_vessels_using.pdf;/home/weberam2/Zotero/storage/8P7EVTS8/mrm.html}
}

