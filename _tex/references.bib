@article{fanQuantitativeOxygenationVenography2014,
  title = {Quantitative Oxygenation Venography from {{MRI}} Phase},
  author = {Fan, Audrey P. and Bilgic, Berkin and Gagnon, Louis and Witzel, Thomas and Bhat, Himanshu and Rosen, Bruce R. and Adalsteinsson, Elfar},
  year = {2014},
  month = jul,
  journal = {Magnetic Resonance in Medicine},
  volume = {72},
  number = {1},
  pages = {149--159},
  issn = {1522-2594},
  doi = {10.1002/mrm.24918},
  abstract = {PURPOSE: To demonstrate acquisition and processing methods for quantitative oxygenation venograms that map in vivo oxygen saturation (SvO2 ) along cerebral venous vasculature. METHODS: Regularized quantitative susceptibility mapping (QSM) is used to reconstruct susceptibility values and estimate SvO2 in veins. QSM with {$\ell$}1 and {$\ell$}2 regularization are compared in numerical simulations of vessel structures with known magnetic susceptibility. Dual-echo, flow-compensated phase images are collected in three healthy volunteers to create QSM images. Bright veins in the susceptibility maps are vectorized and used to form a three-dimensional vascular mesh, or venogram, along which to display SvO2 values from QSM. RESULTS: Quantitative oxygenation venograms that map SvO2 along brain vessels of arbitrary orientation and geometry are shown in vivo. SvO2 values in major cerebral veins lie within the normal physiological range reported by (15) O positron emission tomography. SvO2 from QSM is consistent with previous MR susceptometry methods for vessel segments oriented parallel to the main magnetic field. In vessel simulations, {$\ell$}1 regularization results in less than 10\% SvO2 absolute error across all vessel tilt orientations and provides more accurate SvO2 estimation than {$\ell$}2 regularization. CONCLUSION: The proposed analysis of susceptibility images enables reliable mapping of quantitative SvO2 along venograms and may facilitate clinical use of venous oxygenation imaging.},
  langid = {english},
  pmcid = {PMC4234149},
  pmid = {24006229},
  keywords = {Adult,brain oxygenation,Cerebral Veins,Computer Simulation,Female,Healthy Volunteers,Humans,Image Enhancement,Image Interpretation Computer-Assisted,Imaging Three-Dimensional,Magnetic Resonance Angiography,Male,Oxygen,quantitative susceptibility,venography,venous oxygen saturation},
